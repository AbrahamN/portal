<div class='container'>
  <div id="user_log_in">
    <% if !current_user.nil? %> 
      Logged in as <b><%=current_user.name%></b>
      <a href='/log_out'>
        Log Out
      </a>
    <% else %>
      <a href='log_in'>
        Log In
      </a>
     |
      <a href='sign_up'>
        Register
      </a>
    <%end%>
  </div>
</div>
